{
    "name": "crawl",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "link:assets": "react-native-asset",
        "extract": "node ./scripts/extract-licenses/index.js",
        "android:staging:r": "react-native run-android --mode=stagingrelease",
        "android:staging:d": "react-native run-android --mode=stagingdebug",
        "android:dev:r": "react-native run-android --mode=developmentrelease",
        "android:dev:d": "react-native run-android --mode=developmentdebug",
        "android:prod:r": "react-native run-android --mode=productionrelease",
        "android:prod:d": "react-native run-android --mode=productiondebug",
        "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
        "ios:test": "sh ./scripts/ios-simulator.sh Staging",
        "ios:dev": "sh ./scripts/ios-simulator.sh Dev",
        "ios:prod": "sh ./scripts/ios-simulator.sh Production",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "lint-front": "lint-staged",
        "start": "react-native start --reset-cache",
        "start:reset": "watchman watch-del-all && react-native start --reset-cache",
        "test": "jest",
        "typecheck": "tsc --noEmit",
        "postinstall": "pod-install",
        "deploy:dev": "sh ./scripts/deploy.sh development",
        "deploy:prod": "sh ./scripts/deploy.sh release",
        "link:node_modules": "ln -s ../../node_modules node_modules",
        "delete:node_modules": "rm -rf node_modules",
        "code-push:dev": "yarn link:node_modules && appcenter codepush release-react -a yunchanpark/crawl_ios -d Dev && yarn delete:node_modules",
        "code-push:prod": "yarn link:node_modules && appcenter codepush release-react -a yunchanpark/crawl_ios -d Production && yarn delete:node_modules"
    },
    "dependencies": {
        "@crawl/async-boundary": "workspace:^",
        "@crawl/bottom-sheet": "workspace:^",
        "@crawl/calendar": "workspace:^",
        "@crawl/camera-album": "workspace:^",
        "@crawl/design-system": "workspace:^",
        "@crawl/error-boundary": "workspace:^",
        "@crawl/image-crop": "workspace:^",
        "@crawl/overlay-manager": "workspace:^",
        "@crawl/react-hooks": "workspace:^",
        "@crawl/utils": "workspace:^",
        "@invertase/react-native-apple-authentication": "^2.3.0",
        "@notifee/react-native": "^7.8.2",
        "@react-native-camera-roll/camera-roll": "^7.4.1",
        "@react-native-community/datetimepicker": "^7.6.2",
        "@react-native-community/image-editor": "^3.2.0",
        "@react-native-firebase/app": "^18.8.0",
        "@react-native-firebase/messaging": "^18.8.0",
        "@react-native-google-signin/google-signin": "^11.0.0",
        "@react-native-masked-view/masked-view": "^0.3.1",
        "@react-navigation/bottom-tabs": "^6.5.12",
        "@react-navigation/material-top-tabs": "^6.6.6",
        "@react-navigation/native": "^6.1.10",
        "@react-navigation/native-stack": "^6.9.18",
        "@react-navigation/stack": "^6.3.21",
        "@shopify/flash-list": "^1.6.3",
        "@tanstack/react-query": "^5.20.5",
        "appcenter": "5.0.1",
        "appcenter-analytics": "5.0.1",
        "appcenter-crashes": "5.0.1",
        "compare-versions": "^6.1.0",
        "dayjs": "^1.11.10",
        "expo": "^50.0.6",
        "expo-image": "~1.10.6",
        "expo-secure-store": "~12.8.1",
        "ios-deploy": "^1.12.2",
        "lodash-es": "^4.17.21",
        "pod-install": "^0.2.0",
        "postinstall": "^0.9.0",
        "react": "18.2.0",
        "react-native": "0.73.4",
        "react-native-bootsplash": "^5.4.0",
        "react-native-chart-kit": "^6.12.0",
        "react-native-code-push": "^8.2.1",
        "react-native-config": "^1.5.1",
        "react-native-date-picker": "^4.3.6",
        "react-native-device-info": "^10.12.0",
        "react-native-gesture-handler": "^2.15.0",
        "react-native-haptic-feedback": "^2.2.0",
        "react-native-image-crop-picker": "^0.40.3",
        "react-native-linear-gradient": "^2.8.3",
        "react-native-modal-datetime-picker": "^17.1.0",
        "react-native-pager-view": "^6.2.3",
        "react-native-permissions": "^4.1.1",
        "react-native-progress": "^5.0.1",
        "react-native-reanimated": "^3.7.0",
        "react-native-safe-area-context": "^4.9.0",
        "react-native-screens": "^3.29.0",
        "react-native-skeleton-placeholder": "^5.2.4",
        "react-native-svg": "^14.1.0",
        "react-native-tab-view": "^3.5.2",
        "react-native-webview": "^13.8.1"
    },
    "devDependencies": {
        "@babel/core": "^7.23.2",
        "@babel/preset-env": "^7.22.5",
        "@babel/runtime": "^7.20.0",
        "@faker-js/faker": "^8.0.2",
        "@mswjs/data": "^0.13.0",
        "@react-native/babel-preset": "0.73.21",
        "@react-native/eslint-config": "0.73.2",
        "@react-native/metro-config": "0.73.5",
        "@react-native/typescript-config": "0.73.1",
        "@react-navigation/devtools": "^6.0.19",
        "@tsconfig/react-native": "^3.0.0",
        "@types/jest": "^29.2.1",
        "@types/lodash-es": "^4.17.7",
        "@types/react": "^18.2.6",
        "@types/react-native": "^0.72.2",
        "@types/react-test-renderer": "^18.0.0",
        "babel-jest": "^29.6.3",
        "babel-plugin-module-resolver": "^5.0.0",
        "eslint": "^8.51.0",
        "jest": "^29.6.3",
        "metro-react-native-babel-preset": "0.76.8",
        "msw": "^1.2.3",
        "react-native-asset": "^2.1.1",
        "react-native-url-polyfill": "^2.0.0",
        "react-test-renderer": "18.2.0",
        "typescript": "5.2.2"
    },
    "expo": {
        "autolinking": {
            "exclude": [
                "expo-keep-awake",
                "expo-font",
                "expo-application"
            ]
        }
    },
    "engines": {
        "node": ">=18"
    },
    "packageManager": "yarn@4.1.0"
}
