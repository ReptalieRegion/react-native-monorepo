{
    "name": "reptile-region",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "link:assets": "react-native-asset",
        "android": "react-native run-android",
        "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
        "ios": "sh ./scripts/ios-simulator.sh",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "lint-front": "lint-staged",
        "set:env:test": "sh ./scripts/set-env.sh test",
        "set:env:dev": "sh ./scripts/set-env.sh development",
        "set:env:prod": "sh ./scripts/set-env.sh production",
        "start": "sh ./scripts/start.sh",
        "test": "jest",
        "typecheck": "tsc --noEmit",
        "postinstall": "react-native setup-ios-permissions && pod-install"
    },
    "dependencies": {
        "@hookform/resolvers": "^3.3.0",
        "@react-native-async-storage/async-storage": "^1.19.0",
        "@react-native-camera-roll/camera-roll": "^5.6.0",
        "@react-native-masked-view/masked-view": "^0.2.9",
        "@react-navigation/bottom-tabs": "^6.5.8",
        "@react-navigation/devtools": "^6.0.19",
        "@react-navigation/native": "^6.1.6",
        "@react-navigation/native-stack": "^6.9.12",
        "@react-navigation/stack": "^6.3.17",
        "@reptile-region/bottom-sheet": "1.0.0",
        "@reptile-region/design-system": "1.0.0",
        "@reptile-region/error-boundary": "1.0.0",
        "@reptile-region/overlay-manager": "1.0.0",
        "@shopify/flash-list": "^1.6.1",
        "@tanstack/react-query": "^4.32.0",
        "expo": "^49.0.8",
        "expo-image": "^1.3.2",
        "install-expo-modules": "^0.6.3",
        "ios-deploy": "^1.12.2",
        "lodash-es": "^4.17.21",
        "pod-install": "^0.1.38",
        "postinstall": "^0.8.0",
        "react": "18.2.0",
        "react-hook-form": "^7.45.4",
        "react-native": "0.72.6",
        "react-native-dotenv": "^3.4.9",
        "react-native-gesture-handler": "^2.13.1",
        "react-native-haptic-feedback": "^2.2.0",
        "react-native-linear-gradient": "^2.8.3",
        "react-native-permissions": "^3.9.3",
        "react-native-reanimated": "3.5.4",
        "react-native-safe-area-context": "^4.7.2",
        "react-native-screens": "^3.22.1",
        "react-native-skeleton-placeholder": "^5.2.4",
        "react-native-svg": "^13.14.0",
        "zod": "^3.22.4",
        "zustand": "^4.4.3"
    },
    "devDependencies": {
        "@babel/core": "^7.23.2",
        "@babel/preset-env": "^7.22.5",
        "@babel/runtime": "^7.20.0",
        "@faker-js/faker": "^8.0.2",
        "@mswjs/data": "^0.13.0",
        "@react-native/eslint-config": "^0.72.2",
        "@react-native/metro-config": "^0.72.11",
        "@tsconfig/react-native": "^3.0.0",
        "@types/jest": "^29.2.1",
        "@types/lodash-es": "^4.17.7",
        "@types/react": "^18.0.24",
        "@types/react-native": "^0.72.2",
        "@types/react-test-renderer": "^18.0.0",
        "babel-jest": "^29.7.0",
        "babel-plugin-module-resolver": "^5.0.0",
        "eslint": "^8.51.0",
        "jest": "^29.2.1",
        "metro-react-native-babel-preset": "0.76.8",
        "msw": "^1.2.3",
        "react-native-asset": "^2.1.1",
        "react-native-flipper": "^0.182.0",
        "react-native-url-polyfill": "^2.0.0",
        "react-query-native-devtools": "^4.0.0",
        "react-test-renderer": "18.2.0",
        "typescript": "5.2.2"
    },
    "expo": {
        "autolinking": {
            "exclude": [
                "expo-keep-awake",
                "expo-font",
                "expo-application"
            ]
        }
    },
    "reactNativePermissionsIOS": [
        "Camera",
        "PhotoLibrary",
        "PhotoLibraryAddOnly"
    ],
    "engines": {
        "node": ">=16"
    },
    "packageManager": "yarn@3.6.0"
}
