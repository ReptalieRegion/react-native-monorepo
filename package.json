{
    "name": "reptile-region-react-native",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "android": "react-native run-android",
        "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
        "ios": "sh ./shell/ios-simulator.sh",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "lint-front": "lint-staged",
        "set:env:local": "sh ./shell/set-env.sh local",
        "set:env:dev": "sh ./shell/set-env.sh development",
        "set:env:prod": "sh ./shell/set-env.sh production",
        "start": "react-native start --reset-cache",
        "start:local": "yarn set:env:local && NODE_ENV=local yarn start",
        "start:development": "yarn set:env:dev && NODE_ENV=development yarn start",
        "start:production": "yarn set:env:prod && NODE_ENV=production yarn start",
        "test": "jest",
        "typecheck": "tsc --noEmit",
        "postinstall": "react-native setup-ios-permissions && pod-install"
    },
    "dependencies": {
        "@react-native-async-storage/async-storage": "^1.19.0",
        "@react-native-camera-roll/camera-roll": "^5.6.0",
        "@react-native-masked-view/masked-view": "^0.2.9",
        "@react-navigation/bottom-tabs": "^6.5.8",
        "@react-navigation/devtools": "^6.0.19",
        "@react-navigation/native": "^6.1.6",
        "@react-navigation/native-stack": "^6.9.12",
        "@react-navigation/stack": "^6.3.17",
        "@reptalieregion/webview-bridge": "1.0.25",
        "@shopify/flash-list": "^1.5.0",
        "@tanstack/react-query": "^4.32.0",
        "ios-deploy": "^1.12.2",
        "lodash-es": "^4.17.21",
        "pod-install": "^0.1.38",
        "postinstall": "^0.8.0",
        "react": "18.2.0",
        "react-native": "0.72.3",
        "react-native-dotenv": "^3.4.9",
        "react-native-fast-image": "^8.6.3",
        "react-native-gesture-handler": "^2.12.0",
        "react-native-haptic-feedback": "^2.0.3",
        "react-native-linear-gradient": "^2.8.0",
        "react-native-permissions": "^3.8.0",
        "react-native-reanimated": "3.4.2",
        "react-native-safe-area-context": "^4.6.4",
        "react-native-screens": "^3.22.1",
        "react-native-skeleton-placeholder": "^5.2.4",
        "react-native-svg": "^13.9.0",
        "react-native-webview": "^13.2.1",
        "zustand": "^4.3.8"
    },
    "devDependencies": {
        "@babel/core": "^7.20.0",
        "@babel/preset-env": "^7.22.5",
        "@babel/runtime": "^7.20.0",
        "@faker-js/faker": "^8.0.2",
        "@react-native/eslint-config": "^0.72.2",
        "@react-native/metro-config": "^0.72.9",
        "@tsconfig/react-native": "^3.0.0",
        "@types/jest": "^29.2.1",
        "@types/lodash-es": "^4.17.7",
        "@types/react": "^18.0.24",
        "@types/react-native": "^0.72.2",
        "@types/react-test-renderer": "^18.0.0",
        "babel-jest": "^29.2.1",
        "babel-plugin-module-resolver": "^5.0.0",
        "eslint": "^8.19.0",
        "eslint-plugin-import": "^2.28.0",
        "husky": "^8.0.3",
        "jest": "^29.2.1",
        "lint-staged": "^13.2.3",
        "metro-react-native-babel-preset": "0.76.7",
        "msw": "^1.2.3",
        "prettier": "^2.4.1",
        "react-native-flipper": "^0.182.0",
        "react-native-url-polyfill": "^2.0.0",
        "react-query-native-devtools": "^4.0.0",
        "react-test-renderer": "18.2.0",
        "typescript": "4.8.4"
    },
    "reactNativePermissionsIOS": [
        "Camera",
        "PhotoLibrary",
        "PhotoLibraryAddOnly"
    ],
    "engines": {
        "node": ">=16"
    },
    "packageManager": "yarn@3.6.0",
    "resolutions": {
        "react-native-reanimated@3.4.1": "patch:react-native-reanimated@npm%3A3.4.1#./.yarn/patches/react-native-reanimated-npm-3.4.1-bf4ac7f6b3.patch"
    }
}
